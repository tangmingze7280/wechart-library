<view class="page">
    <view class="page__hd">
        <view class="page__title">我的收藏</view>
        <view class="page__desc">列表</view>
    </view>
    <view class="page__bd">
        <view class="weui-cells__title">收藏列表</view>
        <view wx:for="{{collections}}">
            <view class="weui-cells weui-cells_after-title" >
                <view class="weui-cell">
                    <view class="weui-cell__bd">{{item.bookCode}}</view>
                    <view class="weui-cell__ft" catchtap="delCollection('{{item.wxCode}}')">取消收藏</view>
                </view>
            </view>
        </view>
    </view>
</view>